{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx --no-warnings server/index.ts",
    "dev:no-check": "cross-env NODE_ENV=development tsx --no-warnings --tsconfig tsconfig.server.json server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "setup:platform-admin": "tsx server/setup-platform-admin.ts",
    "seed:rbac-config": "tsx scripts/seed-rbac-config.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:server": "jest --selectProjects=\"Server Tests\"",
    "test:client": "jest --selectProjects=\"Client Tests\"",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --config .eslintrc.cjs",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix --config .eslintrc.cjs",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.test.yml up --abort-on-container-exit",
    "docker:build": "docker build -t saas-framework .",
    "prepare": "husky",
    "packages:build": "npm run packages:build:email && npm run packages:build:auth && npm run packages:build:logging && npm run packages:build:rbac",
    "packages:build:email": "cd packages/email && npm run build",
    "packages:build:auth": "cd packages/auth && npm run build",
    "packages:build:logging": "cd packages/logging && npm run build",
    "packages:build:rbac": "cd packages/rbac && npm run build",
    "packages:publish": "npm run packages:publish:email && npm run packages:publish:auth && npm run packages:publish:logging && npm run packages:publish:rbac",
    "packages:publish:email": "cd packages/email && npm run publish-package",
    "packages:publish:auth": "cd packages/auth && npm run publish-package",
    "packages:publish:logging": "cd packages/logging && npm run publish-package",
    "packages:publish:rbac": "cd packages/rbac && npm run publish-package",
    "packages:verify": "npm run packages:build && echo 'All packages built successfully!'",
    "publish:dry-run": "npm run packages:build && echo 'Dry run completed - packages ready for publishing'"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
