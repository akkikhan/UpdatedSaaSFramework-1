{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:seed": "tsx scripts/seed-dev-data.ts",
    "setup": "npm install && npm run db:push && npm run db:seed",
    "validate": "tsx scripts/validate-setup.ts",
    "quickstart": "node -e \"console.log('Run: scripts/quick-start.bat (Windows) or scripts/quick-start.sh (Linux/Mac)')\"",
    "clean": "rm -rf dist node_modules",
    "reset": "npm run clean && npm install",
    "health": "curl http://localhost:5000/api/health || echo 'Server not running'",
    "logs": "tail -f logs/app.log 2>/dev/null || echo 'No log file found'",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:api": "tsx test/api-tests.ts",
    "dev:debug": "cross-env NODE_ENV=development DEBUG=true tsx server/index.ts",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "npm run lint && npm run format:check && npm run test",
    "docker:start": "node -e \"const os = require('os'); const script = os.platform() === 'win32' ? 'scripts/docker-dev.bat' : 'scripts/docker-dev.sh'; require('child_process').spawn(script, ['start'], {stdio: 'inherit', shell: true});\"",
    "docker:stop": "node -e \"const os = require('os'); const script = os.platform() === 'win32' ? 'scripts/docker-dev.bat' : 'scripts/docker-dev.sh'; require('child_process').spawn(script, ['stop'], {stdio: 'inherit', shell: true});\"",
    "docker:logs": "node -e \"const os = require('os'); const script = os.platform() === 'win32' ? 'scripts/docker-dev.bat' : 'scripts/docker-dev.sh'; require('child_process').spawn(script, ['logs'], {stdio: 'inherit', shell: true});\"",
    "publish:build-all": "tsx scripts/build-all-modules.ts",
    "publish:all": "tsx scripts/publish-all-modules.ts",
    "publish:dev": "tsx scripts/update-dev-versions.ts && npm run publish:build-all && npm run publish:all",
    "publish:version-dev": "tsx scripts/update-dev-versions.ts"
  },
