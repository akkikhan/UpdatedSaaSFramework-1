{
  "name": "@saas-framework/rbac",
  "version": "1.0.0",
  "description": "Role-Based Access Control (RBAC) SDK for SaaS applications with real-time permissions and dynamic policies",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "example-*.js"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "publish-package": "npm publish --access public"
  },
  "keywords": [
    "saas",
    "rbac",
    "role-based-access-control",
    "permissions",
    "authorization",
    "multi-tenant",
    "policies",
    "access-control",
    "security",
    "express"
  ],
  "author": "SaaS Framework Team",
  "license": "MIT",
  "dependencies": {
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "express": ">=4.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saas-framework/rbac"
  },
  "homepage": "https://github.com/saas-framework/rbac#readme",
  "publishConfig": {
    "access": "public"
  }
}
